{
  "manifest_version": 3,
  "name": "Content Adaptive Extension help typing",
  "version": "1.0.0",
  "description": "Welcome to my extension",
  "icons": {
    "16": "assets/16.png",
    "48": "assets/48.png",
    "128": "assets/128.png"
  },
  "action": {
    "default_popup": "UI/popup.html",
    "default_icon": {
      "16": "assets/16.png",
      "48": "assets/48.png",
      "128": "assets/128.png"
    },
    "default_title": "Extension help typing"
  },
  "permissions": [
    "storage",
    "activeTab",
    "scripting",
    "tabs",
    "activeTab"
  ],
  "host_permissions": ["http://192.168.200.26:8090/*","http://localhost:8080/cms/*","https://example.com/*"],
  "web_accessible_resources": [
    {
      "resources": [
        "assets/*",
        "scripts/*",
        "styles/*",
        "UI/*"
      ],
      "matches": [
        "http://192.168.200.26:8090/*"
      ]
    }
  ],
